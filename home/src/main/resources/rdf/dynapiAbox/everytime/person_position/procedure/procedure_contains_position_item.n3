############################################################
#Condition procedure

<https://vivoweb.org/procedure/contains_position_item>
        a                                       dynapi:Procedure ;
        dynapi:providesParameter                iteration_parameter:default_condition_0_int ;
        dynapi:hasFirstStep                     contains_position_item:conditional_step .
        
contains_position_item:conditional_step
        a                                       dynapi:ConditionalStep ;
        dynapi:nextIfSatisfied                  contains_position_item:condition_satisfied_step ;
        dynapi:hasCondition                     contains_position_item:condition_container_contains .
        
contains_position_item:condition_satisfied_step
        a                                       dynapi:OperationalStep ;
        dynapi:hasOperation                     contains_position_item:addOperation .
        
contains_position_item:addOperation
	a                                       dynapi:SumOperation ;
	dynapi:requiresParameter                iteration_parameter:add_int_1_param ;
	dynapi:requiresParameter                iteration_parameter:default_condition_0_int ;
	dynapi:providesParameter                iteration_parameter:default_condition_0_int .

contains_position_item:condition_container_contains
        a                                       dynapi:ConditionContainerContains ;
        dynapi:targetContainer                  request_parameter:positions ;
        dynapi:requiresParameter                iteration_parameter:iteration_int_param .
